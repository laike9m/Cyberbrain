{
	"name": "cyberbrain",
	"displayName": "Cyberbrain (WIP)",
	"description": "A cyberbrain that helps you remember everything.",
	"version": "0.0.1",
	"publisher": "laike9m",
	"repository": "https://github.com/laike9m/cb-experimental",
	"license": "MIT",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cyberbrain.init",
				"title": "Initialize Cyberbrain"
			}
		]
	},
	"scripts": {
		"unittest": "mocha",
		"copy": "$(npm bin)/rimraf ./out/*.js && $(npm bin)/copyfiles -u 1 ./src/*.js ./out/",
		"vscode:prepublish": "npm run compile",
		"compile": "npm run copy && tsc -p ./",
		"lint": "eslint src --ext ts --fix",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "test && mocha && node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/google-protobuf": "^3.7.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.16",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"copyfiles": "^2.3.0",
		"glob": "^7.1.6",
		"hamjest": "^3.7.1",
		"mocha": "^7.2.0",
		"prettier-eslint-cli": "^5.0.0",
		"ts-protoc-gen": "^0.12.0",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.1.5",
		"google-auth-library": "^6.0.6",
		"google-protobuf": "^3.13.0",
		"grpc-tools": "^1.9.1",
		"randomcolor": "^0.6.2",
		"vis-network": "^8.3.2"
	},
	"type": "module"
}
