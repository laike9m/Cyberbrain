{
	"name": "cyberbrain",
	"displayName": "Cyberbrain (WIP)",
	"description": "A cyberbrain that helps you remember everything.",
	"version": "0.0.1",
	"publisher": "laike9m",
	"repository": "https://github.com/laike9m/cb-experimental",
	"license": "MIT",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cyberbrain.init",
				"title": "Initialize Cyberbrain"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
